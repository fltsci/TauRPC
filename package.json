{
  "name": "@fltsci/taurpc",
  "version": "1.8.2-canary.0",
  "description": "Type-safe IPC layer for Tauri commands and events; forked from MatsDK/TauRPC",
  "main": "dist/index.cjs",
  "repository": {
    "url": "https://github.com/fltsci/TauRPC",
    "type": "git"
  },
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    }
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsx watch src/index.ts",
    "format": "dprint fmt",
    "lint": "run-p lint:*",
    "lint:format": "dprint check",
    "lint:types": "tsc",
    "lint:js": "eslint ."
  },
  "keywords": [],
  "contributors": ["MatsDK", "johncarmack1984"],
  "license": "ISC",
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.29.7",
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "dprint": "^0.50.2",
    "eslint": "^9.39.1",
    "eslint-plugin-dprint": "^0.4.0",
    "globals": "^16.5.0",
    "npm-run-all": "^4.1.5",
    "tsup": "^8.5.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0",
    "vitest": "^4.0.13"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.9.0"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "dts": {
      "resolve": true
    },
    "external": [
      ".taurpc"
    ],
    "splitting": true,
    "clean": true
  },
  "packageManager": "pnpm@10.23.0"
}
