{
  "name": "taurpc",
  "version": "1.8.1",
  "description": "",
  "main": "dist/index.cjs",
  "repository": {
    "url": "https://github.com/MatsDK/TauRPC",
    "type": "git"
  },
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    }
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsx watch src/index.ts",
    "format": "dprint fmt",
    "lint": "run-p lint:*",
    "lint:format": "dprint check",
    "lint:types": "tsc",
    "lint:js": "eslint ."
  },
  "keywords": [],
  "author": "MatsDK",
  "license": "ISC",
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.29.5",
    "@types/eslint": "^9.6.1",
    "@types/node": "^24.2.1",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "dprint": "^0.50.1",
    "eslint": "^9.33.0",
    "eslint-plugin-dprint": "^0.4.0",
    "npm-run-all": "^4.1.5",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.7.0"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "dts": {
      "resolve": true
    },
    "external": [
      ".taurpc"
    ],
    "splitting": true,
    "clean": true
  },
  "packageManager": "pnpm@10.14.0"
}
